{
  "name": "customUser",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": false
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "events": {
      "type": "hasMany",
      "model": "event",
      "foreignKey": "participantId",
      "through": "participation"
    },
    "hostedEvents": {
      "type": "hasMany",
      "model": "event",
      "foreignKey": "ownerId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
